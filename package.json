{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint-staged": "lint-staged"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "keywords": [],
  "author": {
    "name": "ccforeverd",
    "email": "zh1045456074@163.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./build/webpack.main.config.js",
            "renderer": {
              "config": "./build/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "name": "main_window",
                  "html": "./src/public/index.html",
                  "js": "./src/windows/main/renderer.ts",
                  "preload": {
                    "js": "./src/windows/main/preload.ts"
                  }
                },
                {
                  "name": "tabs_window",
                  "html": "./src/windows/tabs/index.html",
                  "js": "./src/windows/tabs/renderer.ts",
                  "preload": {
                    "js": "./src/windows/tabs/preload.ts"
                  }
                },
                {
                  "name": "excel_window",
                  "html": "./src/public/index.html",
                  "js": "./src/windows/excel/renderer.ts",
                  "preload": {
                    "js": "./src/windows/excel/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "css-loader": "^3.0.0",
    "deepmerge": "^4.2.2",
    "electron": "13.6.6",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^4.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "lint-staged": "^10.1.7",
    "node-loader": "^0.6.0",
    "pre-commit": "^1.2.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@mdi/font": "^5.1.45",
    "electron-squirrel-startup": "^1.0.0",
    "electron-tabs": "^0.15.0",
    "element-ui": "^2.13.1",
    "roboto-fontface": "^0.10.0",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.4.1",
    "vuetify": "^2.2.23"
  }
}
